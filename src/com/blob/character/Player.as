package com.blob.character{	import Box2DAS.Collision.b2WorldManifold;	import Box2DAS.Common.V2;	import Box2DAS.Dynamics.ContactEvent;	import Box2DAS.Dynamics.StepEvent;		import com.blob.StaticBox;		import flash.events.Event;	import flash.utils.setTimeout;		import misc.Input;	import misc.ScrollerChild;		import shapes.Box;		import wck.BodyShape;	import wck.ContactList;		public class Player extends ScrollerChild	{				public var body:BodyShape;		public var legA:BodyShape;		public var legB:BodyShape;		public var legC:BodyShape;		public var legD:BodyShape;						public var rollTorque:Number = 13.0;		public var maxWheelSpeed:Number = 200.0;		public var wheelSpeedInc:Number = 10.0;				public function Player()		{			super.create();			listenWhileVisible(this, Event.ENTER_FRAME, parseInput, false, 10);			body = getChildByName('body') as BodyShape;			legA = getChildByName('legA') as BodyShape;			legB = getChildByName('legB') as BodyShape;			legC = getChildByName('legC') as BodyShape;			legD = getChildByName('legD') as BodyShape;		}				public function parseInput(e:Event):void {			var left:Boolean = Input.kd('A', 'LEFT');			var right:Boolean = Input.kd('D', 'RIGHT');			var up:Boolean = Input.kd('W', 'UP');			var down:Boolean = Input.kd('S', 'DOWN');						var direction:int = Input.kd('SPACE', 'DOWN') ? 1:-1;						if(left) {				legA.b2body.m_angularVelocity = 20*direction;			}						if(right) {				legB.b2body.m_angularVelocity = 20*direction;			}						if(up) {				legC.b2body.m_angularVelocity = 20*direction;			}						if(down) {				legD.b2body.m_angularVelocity = 20*direction;			}		}	}}